    "types": "tsc -p ./types.json",
    "autotypes": "tsc -p ./types.json -w",
    "start": "sirv dist --single",
    "start:dev": "sirv dist --single --dev",
    "test": "nightwatch --env chrome && nightwatch --env firefox"
